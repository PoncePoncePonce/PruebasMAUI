<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="PruebasMAUI.Pages.Select"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Select">
    <VerticalStackLayout BackgroundColor="White" VerticalOptions="Center">
        <Button
            x:Name="btn_RegistrarSwipe"
            Margin="10"
            BackgroundColor="RoyalBlue"
            Clicked="RegistrarSwipe_Clicked"
            Text="Registrar Pedidos Swipe"
            TextColor="White" />
        <Button
            x:Name="btn_RegistrarSelect"
            Margin="10"
            BackgroundColor="RoyalBlue"
            Clicked="RegistrarSelect_Clicked"
            Text="Registrar Pedidos Select"
            TextColor="White" />
    </VerticalStackLayout>
</ContentPage>